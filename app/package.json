{
  "name": "app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://roicorescabaleiro.github.io/roademy/",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:prod": "vite build",
    "build:dev": "vite build --mode development",
    "cap:dev": "copy .\\capacitor.config.dev.json .\\capacitor.config.json && npm run build:dev && npx cap sync android",
    "cap:prod": "copy .\\capacitor.config.prod.json .\\capacitor.config.json && npm run build:prod && npx cap sync android",
    "build:dev:apk": "npm run cap:dev && cd android && .\\gradlew assembleDebug && cd ..",
    "build:prod:apk": "npm run cap:prod && cd android && .\\gradlew assembleRelease && cd ..",
    "lint": "eslint .",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.2",
    "@capacitor/core": "^7.4.2",
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-slot": "^1.2.3",
    "@tailwindcss/vite": "^4.1.11",
    "axios": "^1.10.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.542.0",
    "prop-types": "^15.8.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^6.0.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.6.2",
    "recharts": "^2.15.4",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.4.2",
    "@eslint/js": "^9.25.0",
    "@tailwindcss/typography": "^0.5.16",
    "@types/react": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "gh-pages": "^6.3.0",
    "globals": "^16.0.0",
    "tw-animate-css": "^1.3.7",
    "vite": "^6.3.5"
  }
}
